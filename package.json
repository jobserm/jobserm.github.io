    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@vue/cli-plugin-eslint": "4.1.1",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-service": "4.1.1",
    "babel-eslint": "^10.0.3",
    "chakra-loader": "^1.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "emotion",
    "vue",
    "chakra-ui",
    "chakra",
    "chakra-ui/vue"
  ]
}
